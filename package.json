{
  "name": "mhews",
  "private": true,
  "scripts": {
    "start": "meteor run --settings=settings.json",
    "start_android": "meteor run android-device --settings=settings.json",
    "test": "meteor test --settings=settings.json --driver-package=practicalmeteor:mocha",
    "stage_server": "(cd private/deploy_scripts; ./stage_server.sh)",
    "release_server": "(cd private/deploy_scripts; ./release_server.sh)",
    "stage_mobile": "(cd private/deploy_scripts; ./stage_mobile.sh)",
    "release_mobile": "(cd private/deploy_scripts; ./release_mobile.sh)",
    "lint": "eslint ."
  },
  "dependencies": {
    "babel-code-frame": "^6.16.0",
    "babel-core": "^6.16.0",
    "babel-runtime": "6.18.0",
    "blueimp-md5": "^2.6.0",
    "d": "^0.1.1",
    "geolib": "^2.0.22",
    "i18next": "^4.1.0",
    "ip": "^1.1.4",
    "lodash": "^4.17.4",
    "material-ui": "^0.16.6",
    "meteor-node-stubs": "~0.2.0",
    "react": "^15.4.1",
    "react-addons-pure-render-mixin": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-google-maps": "^6.0.1",
    "react-i18next": "^1.10.1",
    "react-motion": "^0.4.7",
    "react-mounter": "^1.2.0",
    "react-router": "^3.0.0",
    "react-swipeable-views": "^0.9.3",
    "react-swipeable-views-utils": "^0.9.2",
    "react-tap-event-plugin": "^2.0.0",
    "request": "^2.79.0",
    "sprintf-js": "^1.0.3",
    "suncalc": "^1.8.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "eslint": "^3.5.0",
    "eslint-import-resolver-meteor": "^0.3.3",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-meteor": "^4.0.0",
    "eslint-plugin-react": "^6.2.1",
    "eslint-plugin-mocha": "^4.8.0",
    "sinon": "^1.17.7",
  },
  "uglifyjs2": {
    "deadCodes": [
      "_meteor.Meteor.isServer",
      "Meteor.isServer"
    ],
    "fileRemoval": [
      "packages/ddp-server.js",
      "packages/shell-server.js",
      "packages/ssrwpo_uglifyjs2.js",
      "practicalmeteor:chai",
      "practicalmeteor:sinon",
      "practicalmeteor:mocha",
      "practicalmeteor:mocha-console-runner",
      "lmieulet:meteor-coverage"      
    ],
    "packageDebug": false,
    "options": {
      "fromString": true,
      "compress": {
        "properties": true,
        "dead_code": true,
        "drop_debugger": true,
        "conditionals": true,
        "comparisons": true,
        "evaluate": true,
        "booleans": true,
        "loops": true,
        "unused": true,
        "hoist_funs": true,
        "if_return": true,
        "join_vars": true,
        "cascade": true,
        "collapse_vars": true,
        "negate_iife": true,
        "pure_getters": true,
        "drop_console": true,
        "keep_fargs": false,
        "keep_fnames": false,
        "passes": 2,
        "global_defs": {
          "UGLYFYJS_DEAD": false
        }
      }
    }
  }
}
